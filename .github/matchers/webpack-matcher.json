{
  "problemMatcher": [
    {
      "owner": "ng-webpack",
      "pattern": [
        {
          // Adapted from https://regexr.com/4ua45
          "regexp": "^ERROR in (.+):(\\d+):(\\d+)\\s-\\s(error|warning)\\s(TS\\d+):\\s(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
