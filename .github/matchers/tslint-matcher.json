{
  "problemMatcher": [
    {
      "owner": "ng-lint",
      "pattern": [
        {
          // Adapted from https://github.com/microsoft/vscode-tslint/blob/94595c1c0a80c3fe8fb4d20b2331040e45341cc5/tslint/package.json#L202-L210
          "regexp": "^(WARNING|ERROR):(\\s+\\(\\S*\\))?\\s+(\\S.*)\\[(\\d+), (\\d+)\\]:\\s+(.*)$",
          "severity": 1,
          "file": 3,
          "line": 4,
          "column": 5,
          "message": 6
        }
      ]
    }
  ]
}
